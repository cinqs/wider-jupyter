{
  "name": "Wider Jupyter",
  "version": "0.0.1",
  "description": "use container-fluid instead of container for bigger viewer",
  "permissions": ["activeTab", "declarativeContent"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
